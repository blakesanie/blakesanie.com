{
  "name": "blakesanie.com",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://blakesanie.com",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "react": "^17.0.1",
    "react-device-detect": "^1.14.0",
    "react-div-100vh": "^0.5.6",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-masonry-component": "^6.2.1",
    "react-pdf": "^5.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-snapshot": "^1.3.0",
    "react-typed": "^1.2.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  },
  "reactSnapshot": {
    "include": [
      "/cs/",
      "/photo/",
      "/gear/",
      "/resume/",
      "/instagram/",
      "/linkedin/",
      "/github/",
      "/blog/"
    ],
    "exclude": [
      "/signup",
      "/other-path/exclude-me/**"
    ],
    "snapshotDelay": 50
  }
}
