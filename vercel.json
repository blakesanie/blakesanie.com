{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm config set registry http://registry.npmjs.org && npm install",
  "outputDirectory": "dist",
  "functions": {
    "api/photo/*.js": {
      "includeFiles": "/photo/**/*.html"
    }
  },
  "rewrites": [
    {
      "source": "/photo/map/:path",
      "destination": "/api/photo/:path?map=true"
    },
    {
      "source": "/photo/:path",
      "destination": "/api/photo/:path"
    }
  ],
  "redirects": [
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/blakesanie/"
    },
    {
      "source": "/instagram",
      "destination": "https://www.instagram.com/blake_sanie/"
    },
    {
      "source": "/gourmet",
      "destination": "https://www.instagram.com/blake_gourmet/"
    },
    { "source": "/github", "destination": "https://github.com/blakesanie" },
    {
      "source": "/balance",
      "destination": "https://music.apple.com/us/album/balance/1478925861"
    },
    { "source": "/twitter", "destination": "https://twitter.com/blakesanie" },
    {
      "source": "/paypal",
      "destination": "https://www.paypal.com/paypalme/blakesanie"
    },
    {
      "source": "/venmo",
      "destination": "https://account.venmo.com/u/Blake-Sanie"
    },
    {
      "source": "/youtube",
      "destination": "https://www.youtube.com/channel/UC5DbrnJtkkXNw-txM7NVWfw"
    },
    {
      "source": "/spotify",
      "destination": "https://open.spotify.com/user/blake_sanie?si=uvhN4kfdTQWy4jnrlhKTVg"
    },
    {
      "source": "/source",
      "destination": "https://github.com/blakesanie/blakesanie.com"
    },
    {
      "source": "/blakeOS",
      "destination": "https://www.notion.so/blakesanie/blakeOS-5ddea8783f0f40088effd7720bf059f5"
    },
    {
      "source": "/strava",
      "destination": "https://www.strava.com/athletes/44274878"
    },
    {
      "source": "/menu",
      "destination": "/menu.pdf"
    },
    {
      "source": "/deploy",
      "destination": "https://vercel.com/blakesanie/blakesanie-com/deployments"
    }
  ]
}
