---
const prod = import.meta.env.MODE === 'production';
---
{prod ? (
<script>
    window.originalConsoleLog = console.log;
    console.log = function () {};
</script>
    )

    :
    (<script>
    window.originalConsoleLog = console.log;
    console.log = function (...args) {
        window.originalConsoleLog('dev log:')
        window.originalConsoleLog(...args);
        };
        </script>)
        }